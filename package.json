    "start": "NODE_ENV=development  webpack-dev-server --config ./webpack.config.js --inline --hot --host 0.0.0.0 --port 3000 ",
    "start-prod": "NODE_ENV=production webpack-dev-server --inline --content-base build/ --history-api-fallback --host 0.0.0.0 --port 3000",
    "build-dev": "NODE_ENV=development webpack --progress -p",
    "build-prod": "NODE_ENV=production webpack --progress -p",
    "build": "NODE_ENV=production webpack --config webpack.prod.config.js --progress  --profile  --colors ",
    "deploy": "npm run build&& gh-pages -d public",
